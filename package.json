{
  "name": "@epikodelabs/actionstack",
  "version": "3.0.16",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "react:serve": "vite --config projects/apps/app2/vite.config.ts",
    "react:build": "vite build --config projects/apps/app2/vite.config.ts",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "minify": "node terser.mjs && npx dts-bundler",
    "bundle": "dts-bundle-generator --config bundle.config.cjs && npm run minify",
    "docs:generate": "typedoc --plugin typedoc-plugin-markdown --out docs/api --hideBreadcrumbs --hidePageHeader",
    "docs:dev": "npm run docs:generate && vitepress dev docs --config vitepress.config.ts",
    "docs:build": "npm run docs:generate && vitepress build docs --config vitepress.config.ts && npm run docs:rewrite",
    "docs:rewrite": "node scripts/rewrite-vitepress-base.mjs"
  },
  "dependencies": {
    "@epikodelabs/streamix": "2.0.27",
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.12",
    "@types/eslint": "^9.6.1",
    "@types/estree": "^1.0.5",
    "@types/jasmine": "~4.3.0",
    "@types/markdown-it": "^13.0.9",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-react": "^5.0.1",
    "dts-bundle-generator": "^9.5.1",
    "jasmine-core": "~4.6.0",
    "ng-packagr": "^17.3.0",
    "strip-json-comments": "^5.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.28.10",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "~5.4.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitepress": "^1.6.4"
  },
  "type": "module"
}

